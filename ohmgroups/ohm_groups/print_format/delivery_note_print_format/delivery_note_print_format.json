 "css": "#body{\n    font-size:14px;\n}\n.master{\n    width:100%;\n    border:1px solid black;\n    font-family:arial;\n    font-size:13px;\n}\n.print-format td{\n    padding: 2px!important; border-spacing: 0!important;  margin:0!important; height:0!important;padding-left: 4px!important;padding-right: 3px!important;\n}\n.master table{\n    font-family:arial;\n    font-size:12px;\n    color:black;\n    width:100%;\n    line-height: 1.1!important;\n}\n\n.right1 tr{\n    border-bottom:1px solid black;\n}\n\n.right1 td{\n    /*border-left:1px solid black;*/\n}\n\n.left1{\n    float:left;\n    line-height: 1!important;\n    width:50%;\n}\n\n.right1{\n    float:right;\n    width:50%;\n    line-height: 1!important;\n}\ntable .hsn{\n    width:100;\n    padding-bottom : 50;\n}\n.hsn tr{\n    border-top:0px solid black;\n}",
 "custom_format": 1,
